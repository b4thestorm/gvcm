<center>
<div class="video-theatre">
<iframe class="youtube-player" type="text/html" width="940" height="385"
  src="http://www.youtube.com/embed/LznSf4ztiqc" frameborder="0">
</iframe>
</div>
</center>
<div class="container">
<hr>
<%=form_for @guest, :url => guest_path do |f|%>
<%= f.text_field :email, placeholder: 'coolkid@gmail.com' %> <%= f.submit 'Join The Vibes', class: "btn btn-success"%>
<%end%>
</br>

  <h2 id="subtitle">Video Vibes</h2>
  <%= link_to 'see more videos', watch_index_path, class: 'thumbnail_link'%>
  <div id="video-section">
  <ul>
    <%@displayed.each do |video|%>
    <li><%= link_to (image_tag(video[1].thumbnail_url)), watch_path(video[0].id)%></li>
    <%end%>
  </ul>
  </div>

<p>Administrate<%=link_to 'signup', new_user_registration_path%></p>
<%=link_to 'Sign In', user_session_path, class: 'btn btn-primary' %>
